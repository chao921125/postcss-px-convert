{
  "name": "postcss-px-convert",
  "version": "1.1.8",
  "description": "A PostCSS plugin and Node tool for converting px to rem or vw, suitable for CSS, JS, Vue, React and other scenarios.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "docs"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chao921125/postcss-px-convert.git"
  },
  "bugs": {
    "url": "https://github.com/chao921125/postcss-px-convert/issues"
  },
  "homepage": "https://github.com/chao921125/postcss-px-convert#readme",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "cp -r docs dist/ && cp README.md dist/",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "postcss",
    "px",
    "rem",
    "vw",
    "css",
    "responsive",
    "vue",
    "react",
    "convert",
    "mobile",
    "flexible"
  ],
  "author": "Your Name <your@email.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.2",
    "postcss": "^8.5.6",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/test/**/*.test.ts"]
  }
}
